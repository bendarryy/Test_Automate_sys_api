# Django environment variables for Railway deployment
# Rename this file to .env and set your actual secrets before deploying

DJANGO_SECRET_KEY=your-production-secret-key
DEBUG=False
ALLOWED_HOSTS=your-railway-app.up.railway.app,localhost,127.0.0.1
DATABASE_URL=your-railway-database-url
# Add your frontend URL if needed for CORS/CSRF
CORS_ALLOWED_ORIGINS=https://your-frontend-domain.com
CSRF_TRUSTED_ORIGINS=https://your-frontend-domain.com
